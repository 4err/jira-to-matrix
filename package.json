{
  "name": "jira-matrix-bot",
  "version": "0.7.0",
  "main": "src/app.ts",
  "license": "WTFPL",
  "engines": {
    "node": ">=13"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "compile:test": "rimraf dist && npm run build && rimraf dist",
    "format": "prettier --write \"{src,test}/**/*.js\"",
    "dev": "nodemon logs/start.js",
    "dev:debug": "nodemon --inspect logs/start.js",
    "lint": "eslint \"{src,test}/**/*.js\"",
    "start": "node src/app.js",
    "start-dev": "ts-node src/app.ts",
    "debug": "nodemon --inspect src/app.js",
    "test-single": "NODE_ENV=test nyc mocha",
    "test": "NODE_ENV=test nyc mocha --timeout 10000  test/* --exit",
    "test-all": "TEST_WATCH=1 npm run test",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:grigori-gru/jira-to-matrix.git"
  },
  "nyc": {
    "exclude": [
      "src/redis-client.js",
      "src/modules/log.js",
      "src/config",
      "test"
    ]
  },
  "pre-commit": [
    "lint",
    "test-all"
  ],
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.6",
    "@types/html-to-text": "^5.1.0",
    "@types/joi": "^14.3.4",
    "@types/luxon": "^1.24.0",
    "@types/marked": "^0.7.4",
    "@types/minimist": "^1.2.0",
    "@types/node": "^14.0.4",
    "@types/node-polyglot": "^0.4.34",
    "@types/ramda": "^0.27.5",
    "@types/redis": "^2.8.21",
    "@types/request-promise-native": "^1.0.17",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "chai": "^4.2.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "latest",
    "fs-extra": "^9.0.0",
    "lodash": "^4.17.15",
    "mocha": "latest",
    "nock": "^11.4.0",
    "node-git-server": "^0.6.1",
    "nodemon": "^2.0.2",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.19.1",
    "proxyquire": "^2.1.3",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@slack/web-api": "^5.2.1",
    "@taoqf/javascript-state-machine": "^3.1.4",
    "body-parser": "^1.19.0",
    "david": "^12.0.0",
    "delay": "^4.3.0",
    "express": "^4.17.1",
    "html-to-text": "^5.1.1",
    "is-image": "^3.0.0",
    "javascript-state-machine": "^3.1.0",
    "joi": "^14.3.1",
    "luxon": "^1.23.0",
    "marked": "^0.8.0",
    "matrix-js-sdk": "mobitel-ltd/matrix-js-sdk",
    "minimist": "^1.2.5",
    "node-polyglot": "^2.4.0",
    "ramda": "^0.27.0",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "rimraf": "^3.0.2",
    "ru-names": "^1.0.7",
    "simple-git": "^1.132.0",
    "tmp-promise": "^2.0.2",
    "winston": "^2.4.4"
  }
}
