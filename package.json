{
  "name": "jira-matrix-bot",
  "version": "0.7.0",
  "main": "src/app.ts",
  "license": "WTFPL",
  "engines": {
    "node": ">=13"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "compile:test": "rimraf dist && npm run build && rimraf dist",
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "dev:debug": "nodemon --inspect logs/start.js",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "prestart": "rimraf dist && npm run build",
    "start": "cross-env NODE_ENV=production node dist/app.js",
    "start-dev": "ts-node src/app.ts",
    "debug": "nodemon --inspect src/app.js",
    "test-single": "NODE_ENV=test nyc mocha --watch $1",
    "test": "cross-env NODE_ENV=test nyc mocha",
    "test-all": "cross-env TEST_WATCH=1 npm run test",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:grigori-gru/jira-to-matrix.git"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/types",
      "src/messengers/matrix-api.ts",
      "test",
      "**/*.d.ts"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.11",
    "@types/express": "^4.17.6",
    "@types/fs-extra": "^11.0.1",
    "@types/html-to-text": "^9.0.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.152",
    "@types/luxon": "^3.3.0",
    "@types/marked": "^4.0.8",
    "@types/minimist": "^1.2.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.13",
    "@types/node-polyglot": "^2.4.2",
    "@types/proxyquire": "^1.3.28",
    "@types/request-promise-native": "^1.0.17",
    "@types/sinon": "^10.0.14",
    "@types/sinon-chai": "^3.2.4",
    "@types/supertest": "^2.0.9",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "chai": "^4.2.0",
    "eslint": "^8.39.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "faker": "latest",
    "fs-extra": "^11.1.1",
    "lodash": "^4.17.19",
    "mocha": "latest",
    "nock": "^13.3.0",
    "node-git-server": "^1.0.0",
    "nodemon": "^2.0.2",
    "nyc": "^15.1.0",
    "prettier": "^2.8.7",
    "sinon": "^15.0.4",
    "sinon-chai": "^3.5.0",
    "source-map-support": "^0.5.19",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@gitbeaker/node": "^35.8.1",
    "@slack/web-api": "^6.8.1",
    "@taoqf/javascript-state-machine": "^3.1.4",
    "axios": "^1.4.0",
    "axios-cache-interceptor": "^1.0.1",
    "body-parser": "^1.19.0",
    "canvas": "^2.6.1",
    "cross-env": "^7.0.2",
    "david": "^12.0.0",
    "delay": "^5.0.0",
    "express": "^4.17.1",
    "html-to-text": "^9.0.5",
    "is-image": "^3.0.0",
    "javascript-state-machine": "^3.1.0",
    "joi": "^17.9.2",
    "js-yaml": "^4.1.0",
    "luxon": "^3.3.0",
    "marked": "^4.3.0",
    "matrix-js-sdk": "^25.0.0",
    "minimist": "^1.2.5",
    "node-polyglot": "^2.4.0",
    "proxyquire": "^2.1.3",
    "ramda": "^0.29.0",
    "redis": "^4.6.6",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.8",
    "rimraf": "^5.0.0",
    "ru-names": "^1.0.7",
    "simple-git": "^3.17.0",
    "tmp-promise": "^3.0.3",
    "winston": "^3.8.2"
  }
}
