{
  "name": "jira-matrix-bot",
  "version": "0.3.0",
  "main": "index.js",
  "license": "WTFPL",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "node src/app.js",
    "start-dev": "NODE_ENV=development node src/app.js",
    "test-single": "NODE_ENV=test nyc mocha",
    "test": "NODE_ENV=test nyc mocha test/* --exit",
    "report-coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:grigori-gru/jira-to-matrix.git"
  },
  "pre-commit": [
    "lint",
    "test"
  ],
  "nyc": {
    "exclude": [
      "src/redis-client.js",
      "src/modules/log.js",
      "src/config",
      "src/timeline-handler/commands/help.js",
      "test"
    ]
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "delay": "^4.1.0",
    "eslint": "^5.13.0",
    "eslint-config-google": "^0.12.0",
    "eslint-plugin-chai-friendly": "0.4.1",
    "faker": "latest",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "nyc": "^13.2.0",
    "pre-commit": "^1.2.2",
    "proxyquire": "^2.1.0",
    "sinon": "^7.2.3",
    "sinon-chai": "^3.3.0"
  },
  "dependencies": {
    "@slack/client": "^4.9.0",
    "@slack/events-api": "^2.1.1",
    "body-parser": "^1.18.3",
    "david": "^11.0.0",
    "express": "^4.16.4",
    "html-to-text": "^4.0.0",
    "javascript-state-machine": "^3.1.0",
    "joi": "^14.3.1",
    "marked": "^0.6.0",
    "matrix-js-sdk": "mobitel-ltd/matrix-js-sdk",
    "node-polyglot": "^2.3.0",
    "ramda": "^0.26.1",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "ru-names": "^1.0.7",
    "winston": "^2.4.0",
    "winston-daily-rotate-file": "^1.7.2"
  }
}
